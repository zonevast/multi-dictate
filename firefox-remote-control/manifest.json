{
  "manifest_version": 2,
  "name": "Remote Browser Control",
  "version": "1.0",
  "description": "Allows remote control of Firefox browser",

  "permissions": [
    "tabs",
    "activeTab",
    "nativeMessaging",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "remote-control@multi-dictate",
      "strict_min_version": "50.0"
    }
  }
}
